<scroll-view scroll-y class="scrollPage">
  <image src='/images/imageIden_index.png' mode='widthFix' class='png' style='width:100%;height:386rpx;'></image>
  <view class="padding-l">
    <button bindtap="doUpload" class="cu-btn block bg-blue margin-tb-sm lg">
      <text class="cuIcon-scan iconfont-spin"></text>点击上传图片</button>
  </view>
  <view bindtap="sound" data-index="{{tag_list[index]}}{{fanyi_tag_list[index]}}" data-id="{{[index]}}" style="{{show_tag}}" wx:for="{{tag_list}}" wx:key="typeId" class="padding-lr bg-white margin-top margin-left-xl margin-right-xl">
    <view class="solid-bottom padding">
      <text class="cuIcon-notification"> {{fanyi_tag_list[index]}}</text>
    </view>
    <view class="padding">{{tag_list[index]}}</view>
  </view>
  <view style="height:10rpx;margin-top:30rpx"></view>
</scroll-view>

