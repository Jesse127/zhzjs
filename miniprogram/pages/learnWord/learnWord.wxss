.page{
  height: 100vh;
  width: 100%;
  display: flex;
  justify-content: center;
  flex-direction: column;
  align-items: center;
  overflow: hidden;
}
.wordImage{
  width: 300rpx;
  height: 300rpx;
  border-radius: 20rpx;
  background-color: white;
  background-size: 300rpx;
  margin-bottom: 40rpx;
}
.content{
  border-radius: 30rpx;
  width: 80%;
  text-align: center;
}
.text{
  font-size: 44rpx;
}
.iconSize{
  font-size: 100rpx;
  margin: 40rpx;
}